	<!-- Bmp -->
	<TYPE id="BMP" name="Bmp">
		<PROPERTIES>
			<PROPERTY name="keepLabel" defaultValue="false" type="boolean" nature="publicationAttribute" category="Generation/Caption">
				<COMMENTS>
					<![CDATA[When true, use the assembler label for the caption.
Otherwise, use the last part of the assembler label for the caption.]]>
				</COMMENTS>
			</PROPERTY>
		</PROPERTIES>
		<PUBLICATION>
			<CONTEXT id="FigureProvider"/>
		</PUBLICATION>
	</TYPE>
