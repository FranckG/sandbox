    <eStructuralFeatures xsi:type="ecore:EReference" name="selectedVersions" upperBound="-1"
        eType="#//InstallationCategory" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="currentVersions" upperBound="-1"
