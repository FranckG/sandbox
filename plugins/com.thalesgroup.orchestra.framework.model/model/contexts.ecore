    <eStructuralFeatures xsi:type="ecore:EReference" name="overridingVariables" upperBound="-1"
        eType="#//OverridingVariable" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="superCategoryCategories"
