//Reinitialisation du login utilisé par le connecteur Doors pour se connecté à Doors
/*

*/
OleAutoObj oDoorsConnecteur

oDoorsConnecteur = oleCreateAutoObject("Orchestra.ConnectorDoors")

OleAutoArgs autoArgs = create

string strDatabase = getenv("DOORSDATA")

put(autoArgs,strDatabase)

oleMethod(oDoorsConnecteur, "ReinitializeDoorsLogin", autoArgs)

delete(autoArgs)

oleCloseAutoObject(oDoorsConnecteur)